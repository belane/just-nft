{
  "name": "just-nft",
  "version": "1.0.0",
  "description": "Simple NFT and an English Auction implementations in solidity for evm blockchains.",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npx hardhat test",
    "compile": "npx hardhat clean && npx hardhat compile",
    "coverage": "npx hardhat clean && npx hardhat coverage",
    "solhint": "npx solhint contracts/**/*.sol"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/belane/just-nft.git"
  },
  "keywords": [
    "nft",
    "token",
    "auction",
    "ethereum",
    "solidity",
    "royalties",
    "erc-721",
    "erc-2981"
  ],
  "author": "belane",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/belane/just-nft/issues"
  },
  "homepage": "https://github.com/belane/just-nft#readme",
  "dependencies": {
    "@openzeppelin/contracts": ">=4.7.3",
    "dotenv": "^16.0.0",
    "hardhat-tracer": "^1.1.0-rc.3"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "chai": "^4.2.0",
    "ethereum-waffle": "^3.0.0",
    "ethers": "^5.0.0",
    "hardhat": "^2.7.0",
    "hardhat-gas-reporter": "^1.0.6",
    "prettier": "^2.5.0",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "0.0.5",
    "solidity-coverage": "^0.7.0"
  }
}
